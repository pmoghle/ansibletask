{
    "Version": "2012-10-17",
    "Id": "s3BucketPolicy",
    "Statement": [
        {
            "Sid": "S3Permissions",
            "Effect": "Allow",
            "Principal": {
                "AWS": "*"
            },
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::ls-bucket",
                "arn:aws:s3:::ls-bucket/*"
            ],
            "Condition": {
                "IpAddress": {
                    "aws:SourceIp": "103.24.125.18"
                }
            }
        }
    ]
}
